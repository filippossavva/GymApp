<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gymapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GymApp">
        <activity
            android:name=".RateActivity"
            android:label="@string/title_activity_rate"
            android:theme="@style/Theme.GymApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SocialMediaActivity"
            android:label="@null" />
        <activity
            android:name=".FacilitiesClassesActivity"
            android:label="Facilities and Classes"
            android:theme="@style/Theme.GymApp.NoActionBar"></activity>
        <activity
            android:name=".SelectedGymActivity"
            android:label="Find your Gym"
            android:theme="@style/Theme.GymApp.NoActionBar" />
        <activity
            android:name=".GymSelectActivity"
            android:label="Find your Gym"
            android:theme="@style/Theme.GymApp.NoActionBar" />
        <activity
            android:name=".RegisterActivity"
            android:label="Register" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="Log In">

        </activity>
        <activity
            android:name=".SelectCityActivity"
            android:label="Choose City of your Gym"
            android:theme="@style/Theme.GymApp.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>