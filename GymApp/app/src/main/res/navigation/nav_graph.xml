<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/welcomeFragment">
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.gymapp.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_citySelectFragment"
            app:destination="@id/citySelectFragment" />
    </fragment>
    <fragment
        android:id="@+id/citySelectFragment"
        android:name="com.example.gymapp.CitySelectFragment"
        android:label="fragment_city_select"
        tools:layout="@layout/fragment_city_select" >
        <action
            android:id="@+id/action_citySelectFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
</navigation>